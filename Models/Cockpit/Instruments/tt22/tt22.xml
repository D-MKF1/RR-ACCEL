<!-- VRF Button -->

<animation>
    <type>translate</type>
    <object-name>but.vfr</object-name>
    <property>instrumentation/transponder/but-pos[0]</property>
    <factor>-0.003</factor>
    <axis>
        <x> 1.0</x>
        <y> 0.0</y>
        <z> 0.0</z>
    </axis>
</animation>

<animation>
  <type>pick</type>
  <object-name>but.vfr</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-swap</command>
      <property>instrumentation/transponder/inputs/digit[0]</property>
      <property>instrumentation/transponder/inputs/digit-vfr-swap[0]</property>
    </binding>
    <binding>
      <command>property-swap</command>
      <property>instrumentation/transponder/inputs/digit[1]</property>
      <property>instrumentation/transponder/inputs/digit-vfr-swap[1]</property>
    </binding>
    <binding>
      <command>property-swap</command>
      <property>instrumentation/transponder/inputs/digit[2]</property>
      <property>instrumentation/transponder/inputs/digit-vfr-swap[2]</property>
    </binding>
    <binding>
      <command>property-swap</command>
      <property>instrumentation/transponder/inputs/digit[3]</property>
      <property>instrumentation/transponder/inputs/digit-vfr-swap[3]</property>
    </binding>
    <binding>
			<command>property-toggle</command>
			<property>sim/sound/switch2</property>
		</binding>
    <binding>
      <command>property-assign</command>
      <property>instrumentation/transponder/but-pos[0]</property>
      <value>1.0</value>
    </binding>
    <mod-up>
      <binding>
        <command>property-assign</command>
        <property>instrumentation/transponder/but-pos[0]</property>
        <value>0.0</value>
      </binding>
    </mod-up>
  </action>
  <condition>
    <equals>
      <property>instrumentation/transponder/fn-active</property>
      <value>0</value>
    </equals>
  </condition>
</animation>

